{
  "name": "backend-skymembership",
  "version": "1.0.0",
  "main": "src/App.ts",
  "license": "MIT",
  "scripts": {
    "lint": "npx eslint . --fix",
    "lint:build": "npx eslint . --fix && yarn build",
    "prepare": "husky",
    "postinstall": "patch-package",
    "build": "tsc && yarn copy-static-files",
    "start": "node dist/server.js",
    "copy-keys": "copyfiles -u 1 './src/uploads/**/*' './src/ThirdParty/BAYARIND_DATA_VA/**/*.{pem,pub}' './src/ThirdParty/NOBU_DATA_OPEN_BANK/**/*.{pem,pub}' './src/ThirdParty/NOBU_DATA_VA/**/*.{pem,pub}'",
    "dev": "yarn prettier:format && ts-node-dev --respawn --transpile-only src/server.ts",
    "migration:generate": "sequelize-cli migration:generate",
    "migrate": "sequelize-cli db:migrate",
    "migration:undo": "sequelize-cli db:migrate:undo",
    "migration:undo:all": "sequelize-cli db:migrate:undo:all",
    "prettier:format": "prettier --config .prettierrc src/**/*.ts --write",
    "copy-static-files": "copyfiles -u 1 './src/uploads/**/*' './src/ThirdParty/**/*.{pem,pub}' && echo 'Files copied!' || echo 'Copy failed!'"

  },
  "lint-staged": {
    "*.ts": "eslint --fix"
  },
  "dependencies": {
    "@eslint/js": "^9.9.1",
    "@sentry/node": "^7.54.0",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.11",
    "@types/validator": "^13.12.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.4.5",
    "eslint": "^9.9.1",
    "eslint-plugin-react": "^7.35.0",
    "express": "^4.19.2",
    "http": "^0.0.1-security",
    "http-status-codes": "^2.3.0",
    "husky": "^9.1.5",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.3.1",
    "moment-timezone": "^0.5.45",
    "mysql2": "^3.10.3",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.3.3",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6",
    "serverless": "^4.1.15",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/sequelize": "^4.28.20",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "copyfiles": "^2.4.1",
    "sequelize-cli": "^6.6.2"
  },
  "resolutions": {
    "wrap-ansi": "5.1.0"
  }
}
